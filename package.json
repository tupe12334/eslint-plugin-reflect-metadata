{
  "name": "eslint-plugin-reflect-metadata",
  "version": "1.0.0",
  "description": "ESLint plugin to ensure reflect-metadata is imported before any other imports",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "spell": "cspell lint '**/*.{ts,js,md,json}' --gitignore",
    "spell:check": "cspell lint '**/*.{ts,js,md,json}' --gitignore",
    "knip": "knip",
    "prepare": "husky",
    "release": "release-it"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "reflect-metadata",
    "decorators",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.28.0",
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@cspell/dict-he": "^4.0.5",
    "@types/eslint": "^9.6.1",
    "@types/estree": "^1.0.8",
    "@vitest/coverage-v8": "^4.0.18",
    "cspell": "^9.6.4",
    "eslint": "^9.39.2",
    "eslint-config-agent": "^1.9.3",
    "eslint-config-publishable-package-json": "^1.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "husky": "^9.1.7",
    "knip": "^5.83.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "release-it": "^19.2.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tupe12334/eslint-plugin-reflect-metadata.git"
  },
  "bugs": {
    "url": "https://github.com/tupe12334/eslint-plugin-reflect-metadata/issues"
  },
  "homepage": "https://github.com/tupe12334/eslint-plugin-reflect-metadata#readme"
}
